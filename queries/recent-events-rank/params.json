{
  "cacheHours": -1,
  "refreshHours": 1,
  "onlyFromCache": true,
  "params": [
    {
      "name": "period",
      "column": "created_at",
      "pattern": "^last_hour|last_day|last_week|last_month$",
      "replace": "created_at >= '2022-04-04 02:59:59' AND created_at < '2022-04-11 02:59:59'",
      "default": "last_hour",
      "enums": [
        "last_hour", "last_day", "last_week", "last_month"
      ]
    }
  ]
}
